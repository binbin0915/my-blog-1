        "dayjs": "^1.9.6",
        "env-cmd": "^10.1.0",
        "express": "^4.17.1",
        "http-proxy-middleware": "^1.0.6",
        "js-cookie": "^2.2.1",
        "less-vars-to-js": "^1.3.0",
        "lodash.clonedeep": "^4.5.0",
        "mini-css-extract-plugin": "^1.0.0",
        "mobx": "^5.15.7",
        "mobx-state-tree": "^3.17.2",
        "next": "^9.5.5",
