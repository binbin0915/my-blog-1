{
    "name": "nextjs-blog",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "env-cmd -e dev -f ./.env-cmdrc.json node server.js",
        "build:qa": "env-cmd -e qa -f ./.env-cmdrc.json next build",
        "build:prod": "env-cmd -e prod -f ./.env-cmdrc.json next build",
        "pm2:qa": "pm2 start pm2.config.js  --env qa",
        "pm2:prod": "pm2 start pm2.config.js --env production",
        "start": "cross-env NEXT_PUBLIC_RUN_ENV=prod  next start -p 3000",
        "start:qa": "cross-env NEXT_PUBLIC_RUN_ENV=qa NODE_ENV=production PORT=3000 node server.js",
        "start:prod": "cross-env NEXT_PUBLIC_RUN_ENV=prod NODE_ENV=production PORT=3000 node server.js",
        "export": "next export"
    },
    "dependencies": {
        "@zeit/next-css": "^1.0.1",
        "@zeit/next-less": "^1.0.1",
        "antd": "^4.8.2",
        "axios": "^0.21.0",
        "babel-plugin-import": "^1.13.1",
        "compression": "^1.7.4",
        "cross-env": "^7.0.2",
        "env-cmd": "^10.1.0",
        "express": "^4.17.1",
        "less": "^3.12.2",
        "less-vars-to-js": "^1.3.0",
        "log4js": "^6.3.0",
        "next": "^10.0.0",
        "nprogress": "^0.2.0",
        "react": "16.13.1",
        "react-dom": "16.13.1",
        "webpack-filter-warnings-plugin": "^1.2.1"
    }
}