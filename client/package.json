    "axios": "^0.20.0",
    "babel-plugin-import": "^1.13.0",
    "compression": "^1.7.4",
    "env-cmd": "^10.1.0",
    "express": "^4.17.1",
    "js-cookie": "^2.2.1",
    "less-vars-to-js": "^1.3.0",
    "mini-css-extract-plugin": "^1.0.0",
    "mobx": "^5.15.7",
    "mobx-state-tree": "^3.17.2",
    "next": "^9.5.5",
    "nprogress": "^0.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "style-loader": "^2.0.0",
    "styled-components": "^5.2.0",
    "swiper": "^6.3.1",
    "webpack-filter-warnings-plugin": "^1.2.1"
  },
